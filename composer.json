	"require": {
		"laravel/framework": "4.2.*",
		"badcow/lorem-ipsum": "dev-master",
		"fzaninotto/faker": "1.5.*@dev"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
